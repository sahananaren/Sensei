{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_ANDROID_VERSION_CODE": "15"
      },
      "prebuildCommand": "npx expo prebuild --platform android --clean --no-install"
    }
  },
  "submit": {
    "production": {}
  }
}
